{
    "extends": [
        "standard",
        "standard-react"
    ],
    "rules": {
        "semi": [
            1,
            "always"
        ],
        "key-spacing": 0,
        "jsx-quotes": [
            2,
            "prefer-single"
        ],
        "max-len": [
            2,
            200,
            2
        ],
        "object-curly-spacing": [
            2,
            "always"
        ]
    }
}